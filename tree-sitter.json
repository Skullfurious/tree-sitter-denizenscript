{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "denizenscript",
      "camelcase": "DenizenScript",
      "title": "DenizenScript",
      "scope": "source.denizenscript",
      "file-types": [
        "dsc"
      ],
      "injection-regex": "^denizenscript$",
      "class-name": "TreeSitterDenizenscript"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "A tree-sitter repository that implements Denizen Script (dsc) support.",
    "authors": [
      {
        "name": "Skullfurious",
        "url": ""
      }
    ],
    "links": {
      "repository": "http://github.com/Skullfurious/tree-sitter-denizenscript",
      "funding": ""
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "java": false,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}